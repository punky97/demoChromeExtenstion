{
    "name": "Consul Clone KV",
    "description": "Clone Consul KeyValue to new Path",
    "version": "1.0",
    "browser_action": {
      "default_icon": "/images/consul.png",
      "default_popup": "popup.html"
    },
    "permissions": [
        "webRequest",
        "storage"
      ],
    "content_scripts": [ 
        { 
        "js": [ "jquery.min.js" ],
            "matches": [ "http://*/*", "https://*/*"]
        }
    ],
    "manifest_version": 2
  }